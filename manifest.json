{
  "name": "Automatic Tabs Volume",
  "description": "",
  "version": "1.0.1",
  "manifest_version": 3,
  "permissions": ["tabCapture", "tabs", ""],
  "background": {
    "service_worker": "./js/index.js",
    "type": "module"
  },
  "action": {
    "default_popup": "./popup/popup.html"
  },
  "icons": {
    "128": "logo/logo-128.png",
    "48": "logo/logo-48.png",
    "32": "logo/logo-32.png",
    "16": "logo/logo-16.png"
  }
}
